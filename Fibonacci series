public class series {
    public static void main(String[] args) {
        int n = 20, num1 = 4, num2 = 3;
        System.out.print("Fibonacci Series of "+n+" numbers:");
        for (int i = 1; i <=n; ++i)
        {
            System.out.print(num1+" ");
            int sumOfPrevTwo = num1 + num2;
            num1 = num2;
            num2 = sumOfPrevTwo;
        }
    }
}
